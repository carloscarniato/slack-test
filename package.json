{
  "name": "Slack Test",
  "version": "1.0.0",
  "description": "Send Notification to Slack Channel trought api route",
  "private": true,
  "type": "module",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "check:types": "tsc --noEmit",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node --esm src/index.ts",
    "start:prod": "ts-node --swc --esm src/index.ts",
    "dev": "nodemon | pino-pretty --colorize",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "eslint": "^8.31.0",
    "nodemon": "^2.0.15",
    "pino-pretty": "^9.0.0",
    "prettier": "^2.8.3",
    "vitest": "^0.29.0"
  },
  "dependencies": {
    "@fastify/autoload": "^5.7.1",
    "@fastify/bearer-auth": "^9.0.0",
    "@fastify/helmet": "^10.1.0",
    "@fastify/sensible": "^5.2.0",
    "@slack/web-api": "^6.8.1",
    "@swc/core": "^1.3.26",
    "dotenv": "^16.0.3",
    "fastify": "^4.11.0",
    "fastify-plugin": "^4.5.0",
    "node-cron": "^3.0.2",
    "pino": "^8.8.0",
    "ternoa-js": "^1.4.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "znv": "^0.3.2",
    "zod": "^3.20.2"
  }
}
